<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iscas.samples.distributed.transaction.seata.tcc.server1.mapper.UserDetailsMapper">

    <insert id="insert" parameterType="com.iscas.samples.distributed.transaction.seata.tcc.server1.po.UserDetails" useGeneratedKeys="true" keyProperty="id">
        insert into user_details(user_id, description)
        values (#{userId}, #{description})
    </insert>

</mapper>
